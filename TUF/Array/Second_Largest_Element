    class Solution {
        public int secondLargestElement(int[] arr) {
            int n = arr.length;

            int max = Integer.MIN_VALUE;
            int smax = Integer.MIN_VALUE;

            for(int ind=0;ind<n;ind++){
                if(max < arr[ind]){
                    smax = max;
                    max = arr[ind];
                }else if(max != arr[ind] && smax < arr[ind]){
                    smax = arr[ind];
                }
            }

            if(smax == Integer.MIN_VALUE){
                return -1;
            }

            return smax;
        }
    }