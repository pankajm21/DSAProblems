class Solution {
    public int removeDuplicates(int[] arr) {
        int n = arr.length;
        int l = 0;
        int r = 1;

        while(r < n){
            if(arr[r] != arr[l]){
                l++;
                arr[l] = arr[r];

            }
            r++;
        }

        return l+1;
    }
}